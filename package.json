{
  "name": "simple-warehouse",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "esnext": "node",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=test vitest run --coverage",
    "pretest:e2e": "npm run build",
    "test:e2e": "cross-env NODE_ENV=test playwright test",
    "test:e2e:ui": "npm run test:e2e -- --ui",
    "prepare": "husky install",
    "scripts:generate-env": "tsx ./scripts/generate-env"
  },
  "dependencies": {
    "@conform-to/react": "^0.9.1",
    "@conform-to/zod": "^0.9.1",
    "@ngneat/falso": "^7.1.1",
    "@prisma/client": "^5.6.0",
    "@remix-run/css-bundle": "^2.3.0",
    "@remix-run/node": "^2.3.0",
    "@remix-run/react": "^2.3.0",
    "@remix-run/serve": "^2.3.0",
    "@remix-run/v1-route-convention": "^0.1.4",
    "@stdlib/string-constantcase": "^0.1.1",
    "commander": "^11.1.0",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "fs-extra": "^11.2.0",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-tsconfig-paths": "^4.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@remix-run/dev": "^2.3.0",
    "@remix-run/eslint-config": "^2.3.0",
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.38.0",
    "husky": "^8.0.0",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.6.2",
    "typescript": "^5.1.6",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  }
}
